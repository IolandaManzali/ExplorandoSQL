-- ANALISANDO OS DADOS 

-- pesquisa pela cultura com a maior producao total por regio:

SELECT t2.nm_regiao, t1.nm_cultura, SUM(t3.vl_producao) AS total_producao
FROM t_cultura t1
JOIN t_relatorio_cultura t3 ON t1.id_cultura = t3.id_cultura
JOIN t_regiao t2 ON t2.cd_regiao = t3.cd_regiao
GROUP BY t2.nm_regiao, t1.nm_cultura
ORDER BY t2.nm_regiao, total_producao DESC;

--regiao de maior produtividade de cafe arabica anual

--ano de 2014
SELECT t2.nm_regiao, AVG(t3.vl_produtividade) AS produtividade_media
FROM t_cultura t1
JOIN t_relatorio_cultura t3 ON t1.id_cultura = t3.id_cultura
JOIN t_regiao t2 ON t2.cd_regiao = t3.cd_regiao
WHERE t1.nm_cultura = 'ARABICA' AND t1.id_ano_safra = 2014
GROUP BY t2.nm_regiao
ORDER BY produtividade_media DESC;

-- ano de 2015
SELECT t2.nm_regiao, AVG(t3.vl_produtividade) AS produtividade_media
FROM t_cultura t1
JOIN t_relatorio_cultura t3 ON t1.id_cultura = t3.id_cultura
JOIN t_regiao t2 ON t2.cd_regiao = t3.cd_regiao
WHERE t1.nm_cultura = 'ARABICA' AND t1.id_ano_safra = 2015
GROUP BY t2.nm_regiao
ORDER BY produtividade_media DESC;

-- ano de 2016
SELECT t2.nm_regiao, AVG(t3.vl_produtividade) AS produtividade_media
FROM t_cultura t1
JOIN t_relatorio_cultura t3 ON t1.id_cultura = t3.id_cultura
JOIN t_regiao t2 ON t2.cd_regiao = t3.cd_regiao
WHERE t1.nm_cultura = 'ARABICA' AND t1.id_ano_safra = 2016
GROUP BY t2.nm_regiao
ORDER BY produtividade_media DESC;

-- ano de 2017
SELECT t2.nm_regiao, AVG(t3.vl_produtividade) AS produtividade_media
FROM t_cultura t1
JOIN t_relatorio_cultura t3 ON t1.id_cultura = t3.id_cultura
JOIN t_regiao t2 ON t2.cd_regiao = t3.cd_regiao
WHERE t1.nm_cultura = 'ARABICA' AND t1.id_ano_safra = 2017
GROUP BY t2.nm_regiao
ORDER BY produtividade_media DESC;

-- ano de 2018
SELECT t2.nm_regiao, AVG(t3.vl_produtividade) AS produtividade_media
FROM t_cultura t1
JOIN t_relatorio_cultura t3 ON t1.id_cultura = t3.id_cultura
JOIN t_regiao t2 ON t2.cd_regiao = t3.cd_regiao
WHERE t1.nm_cultura = 'ARABICA' AND t1.id_ano_safra = 2018
GROUP BY t2.nm_regiao
ORDER BY produtividade_media DESC;

-- ano de 2019
SELECT t2.nm_regiao, AVG(t3.vl_produtividade) AS produtividade_media
FROM t_cultura t1
JOIN t_relatorio_cultura t3 ON t1.id_cultura = t3.id_cultura
JOIN t_regiao t2 ON t2.cd_regiao = t3.cd_regiao
WHERE t1.nm_cultura = 'ARABICA' AND t1.id_ano_safra = 2019
GROUP BY t2.nm_regiao
ORDER BY produtividade_media DESC;

-- ano de 2020
SELECT t2.nm_regiao, AVG(t3.vl_produtividade) AS produtividade_media
FROM t_cultura t1
JOIN t_relatorio_cultura t3 ON t1.id_cultura = t3.id_cultura
JOIN t_regiao t2 ON t2.cd_regiao = t3.cd_regiao
WHERE t1.nm_cultura = 'ARABICA' AND t1.id_ano_safra = 2020
GROUP BY t2.nm_regiao
ORDER BY produtividade_media DESC;

-- ano de 2021
SELECT t2.nm_regiao, AVG(t3.vl_produtividade) AS produtividade_media
FROM t_cultura t1
JOIN t_relatorio_cultura t3 ON t1.id_cultura = t3.id_cultura
JOIN t_regiao t2 ON t2.cd_regiao = t3.cd_regiao
WHERE t1.nm_cultura = 'ARABICA' AND t1.id_ano_safra = 2021
GROUP BY t2.nm_regiao
ORDER BY produtividade_media DESC;

-- ano de 2022
SELECT t2.nm_regiao, AVG(t3.vl_produtividade) AS produtividade_media
FROM t_cultura t1
JOIN t_relatorio_cultura t3 ON t1.id_cultura = t3.id_cultura
JOIN t_regiao t2 ON t2.cd_regiao = t3.cd_regiao
WHERE t1.nm_cultura = 'ARABICA' AND t1.id_ano_safra = 2022
GROUP BY t2.nm_regiao
ORDER BY produtividade_media DESC;

-- ano de 2023
SELECT t2.nm_regiao, AVG(t3.vl_produtividade) AS produtividade_media
FROM t_cultura t1
JOIN t_relatorio_cultura t3 ON t1.id_cultura = t3.id_cultura
JOIN t_regiao t2 ON t2.cd_regiao = t3.cd_regiao
WHERE t1.nm_cultura = 'ARABICA' AND t1.id_ano_safra = 2023
GROUP BY t2.nm_regiao
ORDER BY produtividade_media DESC;

-- ano de 2024

SELECT t2.nm_regiao, AVG(t3.vl_produtividade) AS produtividade_media
FROM t_cultura t1
JOIN t_relatorio_cultura t3 ON t1.id_cultura = t3.id_cultura
JOIN t_regiao t2 ON t2.cd_regiao = t3.cd_regiao
WHERE t1.nm_cultura = 'ARABICA' AND t1.id_ano_safra = 2024
GROUP BY t2.nm_regiao
ORDER BY produtividade_media DESC;


SELECT t2.nm_regiao, t1.nm_cultura, SUM(t3.vl_producao) AS total_producao
FROM t_cultura t1
JOIN t_relatorio_cultura t3 ON t1.id_cultura = t3.id_cultura
JOIN t_regiao t2 ON t2.cd_regiao = t3.cd_regiao
GROUP BY t2.nm_regiao, t1.nm_cultura
ORDER BY t2.nm_regiao, total_producao DESC;

--regiao de maior produtividade de cafe conillon anual

--ano de 2014
SELECT t2.nm_regiao, AVG(t3.vl_produtividade) AS produtividade_media
FROM t_cultura t1
JOIN t_relatorio_cultura t3 ON t1.id_cultura = t3.id_cultura
JOIN t_regiao t2 ON t2.cd_regiao = t3.cd_regiao
WHERE t1.nm_cultura = 'CONILLON' AND t1.id_ano_safra = 2014
GROUP BY t2.nm_regiao
ORDER BY produtividade_media DESC;

