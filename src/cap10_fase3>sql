-- ATIVIDADE "EXPLORANDO SQL E TIPOS DE DADOS ORACLE" 

DROP TABLE t_cultura CASCADE CONSTRAINTS;

DROP TABLE t_regiao CASCADE CONSTRAINTS;

DROP TABLE t_relatorio_cultura CASCADE CONSTRAINTS;

-- predefined type, no DDL - MDSYS.SDO_GEOMETRY

-- predefined type, no DDL - XMLTYPE

  
-- criando as tabelas e constrains 
CREATE TABLE t_cultura (
    id_cultura   VARCHAR2(10) NOT NULL,
    cd_regiao    VARCHAR2(10) NOT NULL,
    cd_relatorio VARCHAR2(10) NOT NULL,
    nm_cultura   VARCHAR2(100) NOT NULL,
    id_ano_safra NUMBER(4) NOT NULL
)
LOGGING;

ALTER TABLE t_cultura ADD CONSTRAINT pk_t_cultura_pk PRIMARY KEY ( id_cultura );

CREATE TABLE t_regiao (
    cd_regiao VARCHAR2(10) NOT NULL,
    sg_regiao CHAR(2) NOT NULL,
    nm_regiao VARCHAR2(50) NOT NULL
)
LOGGING;

ALTER TABLE t_regiao ADD CONSTRAINT pk_t_regiao_pk PRIMARY KEY ( cd_regiao );

CREATE TABLE t_relatorio_cultura (
    cd_relatorio     VARCHAR2(10) NOT NULL,
    id_cultura       VARCHAR2(10) NOT NULL,
    cd_regiao        VARCHAR2(10) NOT NULL,
    ar_plantada_ep   NUMBER(30, 2) NOT NULL,
    ar_plantada_em   NUMBER(30, 2) NOT NULL,
    vl_produtividade NUMBER(30, 2) NOT NULL,
    vl_producao      NUMBER(30, 2) NOT NULL
)
LOGGING;

ALTER TABLE t_relatorio_cultura ADD CONSTRAINT pk_t_relatorio_cultura_pk PRIMARY KEY ( cd_relatorio );

ALTER TABLE t_cultura
    ADD CONSTRAINT fk_cultura_regiao FOREIGN KEY ( cd_regiao )
        REFERENCES t_regiao ( cd_regiao )
    NOT DEFERRABLE;

ALTER TABLE t_relatorio_cultura
    ADD CONSTRAINT fk_rel_cultura_cultura FOREIGN KEY ( id_cultura )
        REFERENCES t_cultura ( id_cultura )
    NOT DEFERRABLE;

ALTER TABLE t_relatorio_cultura
    ADD CONSTRAINT fk_rel_cultura_regiao FOREIGN KEY ( cd_regiao )
        REFERENCES t_regiao ( cd_regiao )
    NOT DEFERRABLE;

