-- evolucao da producao do cafe arabica de 2014 a 2024
SELECT t1.id_ano_safra, SUM(t3.vl_producao) AS total_producao_arabica
FROM t_cultura t1
JOIN t_relatorio_cultura t3 ON t1.id_cultura = t3.id_cultura
WHERE t1.nm_cultura = 'ARABICA'
GROUP BY t1.id_ano_safra
ORDER BY t1.id_ano_safra;

-- evolucao da producao de cafe conillon de 2014 a 2024
SELECT t1.id_ano_safra, SUM(t3.vl_producao) AS total_producao_arabica
FROM t_cultura t1
JOIN t_relatorio_cultura t3 ON t1.id_cultura = t3.id_cultura
WHERE t1.nm_cultura = 'CONILLON'
GROUP BY t1.id_ano_safra
ORDER BY t1.id_ano_safra;

-- comparacao da produtividade media dos cafes arabica e conillon por regiao (valores com 2 casas decimais - utilizando funcao de arredondamento)
SELECT t2.nm_regiao,
       ROUND(AVG(CASE WHEN t1.nm_cultura = 'ARABICA' THEN t3.vl_produtividade END), 2) AS produtividade_arabica,
       ROUND(AVG(CASE WHEN t1.nm_cultura = 'CONILLON' THEN t3.vl_produtividade END), 2) AS produtividade_conillon
FROM t_cultura t1
JOIN t_relatorio_cultura t3 ON t1.id_cultura = t3.id_cultura
JOIN t_regiao t2 ON t2.cd_regiao = t3.cd_regiao
GROUP BY t2.nm_regiao;

--analise da producao dos cafes arabica e conillon por ano
SELECT
    t1.nm_cultura,
    t1.id_ano_safra,
    SUM(t3.vl_producao) AS producao_total
FROM
    t_cultura t1
JOIN t_relatorio_cultura t3 ON t1.id_cultura = t3.id_cultura
GROUP BY
    t1.nm_cultura, t1.id_ano_safra
ORDER BY
    t1.nm_cultura, t1.id_ano_safra;

-- comparacao da produtividade media entre os cafes arabica e conillon (todas as regioes)

--desvio padro
SELECT
    t1.nm_cultura,
    AVG(t3.vl_produtividade) AS produtividade_media
FROM
    t_cultura t1
JOIN t_relatorio_cultura t3 ON t1.id_cultura = t3.id_cultura
GROUP BY
    t1.nm_cultura;


-- analises estatisticas da produtividade do cafe arabica
SELECT 
    t2.nm_regiao, 
    STDDEV(t3.vl_produtividade) AS desvio_padrao_produtividade
FROM 
    t_cultura t1
JOIN t_relatorio_cultura t3 ON t1.id_cultura = t3.id_cultura
JOIN t_regiao t2 ON t2.cd_regiao = t3.cd_regiao
WHERE t1.nm_cultura = 'ARABICA'
GROUP BY t2.nm_regiao;


-- analises estatisticas da produtividade do cafe conillon
--desvio padrao

SELECT 
    t2.nm_regiao, 
    STDDEV(t3.vl_produtividade) AS desvio_padrao_produtividade
FROM 
    t_cultura t1
JOIN t_relatorio_cultura t3 ON t1.id_cultura = t3.id_cultura
JOIN t_regiao t2 ON t2.cd_regiao = t3.cd_regiao
WHERE t1.nm_cultura = 'CONILLON'
GROUP BY t2.nm_regiao;
